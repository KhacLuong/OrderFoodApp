<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/layout_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/define_dimen_10"
    android:layout_marginEnd="@dimen/define_dimen_10"
    app:cardCornerRadius="@dimen/define_dimen_6"
    >
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >
        <TextView
            android:id="@+id/tv_table"
            android:layout_width="@dimen/define_dimen_60"
            android:layout_height="@dimen/define_dimen_60"
            android:text="@string/table"
            android:gravity="center"
            android:background="@drawable/bg_gray_shape_corner_6"
            android:textSize="@dimen/define_dimen_16"
            android:textStyle="normal"
            android:textColor="@color/black"/>

    </LinearLayout>

</androidx.cardview.widget.CardView>